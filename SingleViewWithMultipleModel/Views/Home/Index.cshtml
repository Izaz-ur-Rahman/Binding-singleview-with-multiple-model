@model IEnumerable<SingleViewWithMultipleModel.Models.Student>
@{
    ViewData["Title"] = "Home Page";
}

<h2>Students List</h2>
<div class="container">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Gender</th>
                <th>Standard</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var student in Model)
            {
                <tr>
                    <td>@student.Id</td>
                    <td>@student.Name</td>
                    <td>@student.Gender</td>
                    <td>@student.Standard</td>
                </tr>
            }
        </tbody> 
        
    </table>
</div>